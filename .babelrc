{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "transform-object-assign",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "transform-react-jsx",
      {
        "pragma": "h"
      }
    ],
    [
      "jsx-pragmatic",
      {
        "module": "preact",
        "export": "h",
        "import": "h"
      }
    ]
  ]
}